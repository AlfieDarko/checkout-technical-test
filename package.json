{
  "name": "checkout-technical-test",
  "version": "1.0.0",
  "description": "A customer feedback page for checkout.com technical test",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "start": "webpack-dev-server --mode development --hot --history-api-fallback --open --config webpack.base.config.js",
    "predeploy": "npm run lint && npm run test && webpack --mode production --config webpack.prod.config.js  --progress",
    "deploy": "webpack --mode production --config webpack.prod.config.js  --progress",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfieDarko/checkout-technical-test.git"
  },
  "author": "Alfie Darko <alfie.darko@clearscore.com> (https://www.clearscore.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfieDarko/checkout-technical-test/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "testEnvironment": "enzyme",
    "moduleNameMapper": {
      "^#views(.*)$": "<rootDir>/src/views/$1",
      "^#assets(.*)$": "<rootDir>/src/views/assets/$1",
      "^#context(.*)$": "<rootDir>/src/context/$1",
      "^.+\\.[t|j]sx?$": "babel-jest",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/AlfieDarko/checkout-technical-test#readme",
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-transform-runtime": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "autoprefixer": "9.8.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "3.6.0",
    "cypress": "4.9.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.4.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-react": "7.20.3",
    "file-loader": "6.0.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "jest-environment-enzyme": "7.1.2",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "jest-enzyme": "7.1.2",
    "jest-transform-stub": "2.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "style-loader": "1.2.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "7.10.3",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "5.7.2",
    "recharts": "1.8.5"
  }
}
